  "bugs": {
    "url": "https://github.com/edwardoboh/zc_plugin_company_files/issues"
  },
  "homepage": "https://github.com/edwardoboh/zc_plugin_company_files#readme",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "concurrently": "^6.2.1",
    "nodemon": "^2.0.12"
  }
}
